{
  "compilerOptions": {
    "target": "es2016", 
    "lib": [
      "ESNext",
      "DOM"
    ],
    "module": "commonjs",
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  }
}
